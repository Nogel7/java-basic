package constructor;

public class Webtoon {
	public String name;
	public int series;
	public String painer;
	public boolean complete;
	
	
	public void getinfo() {
		System.out.println("제목은 : " + name + ", 회차는 : " + series + ",  작가는 : " + painer + " , 완결여부는 : " + complete);
	}
    
	public void uploadWebtoon() {
		series += 1;
	}
    
	public void completeWebtoon() {
		complete = true;
	}
}
